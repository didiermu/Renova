- titulo = "Renova Industrial"
- subTituloHero = "Optimizamos el manejo de tus residuos industriales ";
- tituloHero = "con equipo moderno y especializado";

doctype
html(lang='es')

    include includes/head.pug

    body
        include includes/header.pug
        main.home
            include includes/hero.pug
            
            section.ayuda
                svg.path1(width='1000px' height='800px' viewBox='0 0 900 824')
                    g.Page-1(stroke='none' stroke-width='1' fill='none' fill-rule='evenodd')
                        path(d="M584.473023,562.458371 L212.57173,299.650718 C122.365,235.905257 -2.43790882,257.356355 -66.1833689,347.563085 C-83.9353827,372.684107 -95.6536808,401.561328 -100.426977,431.949078 L-182.296541,953.146978 L-182.296541,953.146978" id="Path-5-Copy-2" stroke="#F9BB00" stroke-width="2" transform="translate(201.088241, 533.465513) rotate(-243.000000) translate(-201.088241, -533.465513) ")
                        
                .container-fluid
                    .row.align-items-center.objetFade
                        .col-12.col-lg-5
                            .ayuda--img
                                picture 
                                    source(srcset="img/Desktop/1Home/Secc1.webp", type="image/webp")
                                    img.img-fluid(src="img/Desktop/1Home/Secc1.jpg", alt="Renova Industrial")
                                
                        .col-12.col-lg-7
                            .ayuda--copy.objParallax
                                h2.titleBig
                                    b ¿Cómo te
                                        br 
                                        | podemos ayudar?
                                        
                                .ayuda--copy__acceso.offset-lg-1
                                    h2 Quiero vender mis desperdicios
                                        br 
                                        | industriales
                                    
                                    a(href="#!").linkBorder Conoce nuestros servicios 
                                    
                                .ayuda--copy__acceso.offset-lg-1
                                    h2 Quiero comprar materiales
                                        br 
                                        | reciclados
                                    
                                    a(href="#!").linkBorder Ver materiales
                
            section.dedicamos
                svg.path1(width='800px' height='800px' viewBox='0 0 1000 1100')
                    g.Page-1(stroke='none' stroke-width='1' fill='none' fill-rule='evenodd')
                        
                        path(d="M829.358284,449.674323 L449.519976,181.2579 C359.313246,117.51244 234.510337,138.963537 170.764877,229.170267 C154.089911,252.767149 142.72582,279.696927 137.454748,308.106136 L0.703458895,1045.14698 L0.703458895,1045.14698" id="Path-5-Copy-3" stroke="#F9BB00" stroke-width="2" transform="translate(415.030872, 523.073489) rotate(-180.000000) translate(-415.030872, -523.073489) ")
                        
                .container-fluid
                    .row.align-items-center.objetFade
                        .col-12.col-lg-5.offset-lg-3.offset-xl-4
                            .dedicamos--copy.objParallax
                                h3 Nos dedicamos a la 
                                    br 
                                    | compra, venta y manejo de 
                                h2.titleBig
                                    b residuos 
                                        br 
                                        | industriales
                                h4 Contamos con equipo especializado, 
                                    br 
                                    | personal capacitado y certificaciones 
                                    br
                                    | que nos permiten ofrecer un servicio 
                                    br 
                                    |profesional industriales
                                
                                a(href="#!").linkBorder Conoce Renova
                                        
                                    
                        
                        .dedicamos--img
                            picture 
                                source(srcset="img/Desktop/1Home/Secc2.png", type="image/webp")
                                img.img-fluid(src="img/Desktop/1Home/Secc2.png", alt="Renova Industrial")
            
            section.promos
                img.fondo3(src="img/fondo3.svg", alt="")
                
                svg#path1(width='751px' height='824px' viewBox='0 0 751 824')
                    g#Page-1(stroke='none' stroke-width='1' fill='none' fill-rule='evenodd')
                        path(d="M707.699302,793.745039 L719.820771,230.480114 L387.954512,30.3141528 C293.370261,-26.7345739 170.447445,3.69398073 113.398718,98.2782319 C100.034329,120.435795 91.0966617,144.976215 87.0813138,170.538717 L1.39332472,716.045467 L1.39332472,716.045467" id="Path-5" stroke="#F9BB00" stroke-width="2")
                                        
                .container-fluid
                    .row.align-items-center.objetFade
                        .col-12.promos--img
                            
                        .col-12.col-lg-8.offset-lg-3.offset-xl-4
                            .promos--copy
                                h2.titleBig 
                                    b Promociones    
                                
                                .row
                                    - var n = 0;
                                    while n < 3
                                        - ++n
                                        .col-lg-4
                                            .promos--copy__card
                                                h5 Precio preferencial
                                                h2 En volumen de lámina
                                                a(href="#!") 
                                                    button.btn.secundario Me interesa
                                
                                a(href="#!").linkBorder Ver las promociones 
                                
            section.contacto
                svg.path1(width='700px' height='700px' viewBox='0 0 1250 900')
                    g.Page-1(stroke='none' stroke-width='1' fill='none' fill-rule='evenodd')
                        path(d="M1077.35828,291.674323 L697.519976,23.2578999 C607.313246,-40.4875602 482.510337,-19.0364627 418.764877,71.1702671 C402.089911,94.7671494 390.72582,121.696927 385.454748,150.106136 L248.703459,887.146978 L248.703459,887.146978" id="Path-5-Copy-4" stroke="#F9BB00" stroke-width="2" transform="translate(663.030872, 365.073489) rotate(-302.000000) translate(-663.030872, -365.073489) ")
                        
                       
                .container-fluid
                    .row.align-items-center.objetFade
                        .col-12.col-lg-6.col-xl-5
                            .contacto--img
                                picture 
                                    source(srcset="img/Desktop/1Home/Secc4.png", type="image/webp")
                                    img.img-fluid(src="img/Desktop/1Home/Secc4.png", alt="Renova Industrial")
                                
                                
                        .col-12.col-lg-5
                            .contacto--copy.objParallax
                                h2 Si quieres saber sobre nuestras promociones 
                                    br 
                                    | o solicitar nuestros servicios
                                h2.titleBig
                                    b Escríbenos
                                
                                a(href="#!") 
                                    button.btn.secundario Me interesa
                                                       
        include includes/footer.pug
